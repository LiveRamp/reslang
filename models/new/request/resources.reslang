

request-resource UploadRequest {
    id: int
    name: string
    status: UploadRequestStatusEnum
    client: string

	operations
		POST GET MULTIGET id name
}

singleton subresource UploadRequest::Status {
    id: int
    recordsProcessed: int
    completed: boolean

    operations
        GET
}

subresource UploadRequest::RecordSet {
    id: int
    company: string
    records: Record[]

    operations 
        GET POST
}

structure Record {
    details: string
    address: string
}

action UploadRequest::StartProcessingAction  {
    operations
        POST
}

action UploadRequest::StopProcessingAction {
    id: int
    when: datetime

    operations
        POST GET
}

enum UploadRequestStatusEnum {
    STARTED
    STOPPED
    COMPLETED
}