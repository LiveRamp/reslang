
resource ResourceA {
    id: int
    name: string
    b: linked ResourceB
    newB: linked v2/ResourceB
    
    operations
        GET
}

resource ResourceB {
    id: int
    size: int

    operations
        POST GET PUT
}

resource v2/ResourceB {
    id: int
    totalSize: int

    operations
        POST GET
}

subresource Sub of v2/ResourceB {
    id: int
    name: string

    operations
        POST GET
}