
asset-resource User {
    id: int
    name: string

    /operations
        POST GET
}

sync action User::Suspend {
    id: int
    /operations
        PUT
}

sync action User::Unsuspend {
    id: int
    /operations
        PUT
}

asset-resource Organization {
    id: int
    name: string
    parentOrganizationId: linked Organization optional

    /operations
        POST GET MULTIGET
}

asset-resource Role {
    id: int
    name: string
    parentRoleId: linked Role

    /operations
        POST GET MULTIGET
}