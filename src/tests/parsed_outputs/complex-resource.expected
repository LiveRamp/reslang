[
  {
    "description": "This is a more complex example, featuring\na car with a set of wheel subresources",
    "version": "0.0.1"
  },
  [
    {
      "import": "Garage",
      "file": "simple-resource"
    }
  ],
  [
    {
      "type": "resource",
      "name": "Car",
      "singleton": false,
      "comment": "this is a Car resource ",
      "attributes": [
        {
          "name": "id",
          "type": "int",
          "multiple": false,
          "output": false,
          "linked": false
        },
        {
          "name": "bought",
          "type": "date",
          "multiple": false,
          "output": true,
          "linked": false
        },
        {
          "name": "brand",
          "type": "BrandEnum",
          "multiple": false,
          "output": false,
          "linked": false
        },
        {
          "name": "engine",
          "type": "Specification",
          "multiple": false,
          "output": false,
          "linked": false
        },
        {
          "name": "components",
          "type": "Specification",
          "multiple": true,
          "output": false,
          "linked": false
        },
        {
          "name": "home",
          "type": "Garage",
          "multiple": false,
          "output": false,
          "linked": true
        },
        {
          "name": "rented",
          "type": "Garage",
          "multiple": true,
          "output": false,
          "linked": true
        }
      ],
      "operations": [
        {
          "operation": "POST"
        },
        {
          "operation": "GET",
          "comment": "this is a get comment "
        },
        {
          "operation": "MULTIGET",
          "ids": [
            "id",
            "brand"
          ]
        }
      ]
    },
    {
      "type": "structure",
      "name": "Specification",
      "comment": "this is a specification!",
      "attributes": [
        {
          "name": "type",
          "type": "string",
          "multiple": false,
          "output": false,
          "linked": false
        },
        {
          "name": "documentation",
          "type": "string",
          "multiple": false,
          "output": false,
          "linked": false
        }
      ]
    },
    {
      "type": "subresource",
      "name": "Wheel",
      "singleton": false,
      "parent": "Car",
      "comment": "A wheel is owned by a car",
      "attributes": [
        {
          "name": "id",
          "type": "int",
          "multiple": false,
          "output": false,
          "linked": false
        },
        {
          "name": "radius",
          "type": "double",
          "multiple": false,
          "output": false,
          "linked": false
        }
      ],
      "operations": [
        {
          "operation": "GET"
        },
        {
          "operation": "PUT"
        },
        {
          "operation": "POST"
        },
        {
          "operation": "MULTIGET",
          "ids": [
            "id"
          ]
        }
      ]
    },
    {
      "type": "enum",
      "name": "BrandEnum",
      "literals": [
        "TOYOTA",
        "FORD"
      ]
    }
  ]
]